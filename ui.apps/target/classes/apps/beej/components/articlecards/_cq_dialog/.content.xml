<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Articles"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}false">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Properties"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <featureSliceTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Feature Slice Title"
                                                    name="./featureSliceTitle"/>
                                            <alignment
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="Feature Alignment"
                                                    name="./featureAlignment">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <left
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Left Align (Default)"
                                                            value=""/>
                                                    <center
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Center Align"
                                                            value="beej-feature-slice__feature--center-align"/>
                                                    <right
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Right Align"
                                                            value="beej-feature-slice__feature--right-align"/>
                                                </items>
                                            </alignment>
                                            <iconSize
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                    fieldLabel="Icon Size"
                                                    name="./featureIconSize">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <default
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Large (Default)"
                                                            value=""/>
                                                    <small
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Small"
                                                            value="beej-feature-slice__feature-icon--small"/>
                                                </items>
                                            </iconSize>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <featureOne
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Feature 1"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <featureIcon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                    allowUpload="{Boolean}false"
                                                    autoStart="{Boolean}false"
                                                    class="cq-droptarget"
                                                    fieldLabel="Feature Icon"
                                                    fieldDescription="Drop an icon from the DAM here"
                                                    fileNameParameter="./primaryLogo"
                                                    fileReferenceParameter="./featureIcon1"
                                                    mimeTypes="[image]"
                                                    multiple="{Boolean}false"
                                                    name="./file"
                                                    title="Primary Logo Upload"
                                                    useHTML5="{Boolean}true"/>
                                            <featureTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Feature Title"
                                                    name="./featureTitle1"/>
                                            <featureText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    fieldLabel="Feature Text"
                                                    name="./featureText1"/>
                                            <ctaURL
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldLabel="Feature CTA Path"
                                                    name="./featureCTAPath1"
                                                    rootPath="/content"/>
                                            <ctaText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Feature CTA Text"
                                                    name="./featureCTAText1"/>
                                            <ctaColor
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="CTA Color"
                                                    name="./featureCTAColor1">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <default
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Default (Default)"
                                                            value=""/>
                                                    <pink
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Pink"
                                                            value="beej-button--pink"/>
                                                    <blue
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Blue"
                                                            value="beej-button--blue"/>
                                                    <purple
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Purple"
                                                            value="beej-button--purple"/>
                                                </items>
                                            </ctaColor>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </featureOne>
                    <featureTwo
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Feature 2"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <featureIcon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                    allowUpload="{Boolean}false"
                                                    autoStart="{Boolean}false"
                                                    class="cq-droptarget"
                                                    fieldLabel="Feature Icon"
                                                    fieldDescription="Drop an icon from the DAM here"
                                                    fileNameParameter="./primaryLogo"
                                                    fileReferenceParameter="./featureIcon2"
                                                    mimeTypes="[image]"
                                                    multiple="{Boolean}false"
                                                    name="./file"
                                                    title="Primary Logo Upload"
                                                    useHTML5="{Boolean}true"/>
                                            <featureTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Feature Title"
                                                    name="./featureTitle2"/>
                                            <featureText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    fieldLabel="Feature Text"
                                                    name="./featureText2"/>
                                            <ctaURL
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldLabel="Feature CTA Path"
                                                    name="./featureCTAPath2"
                                                    rootPath="/content"/>
                                            <ctaText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Feature CTA Text"
                                                    name="./featureCTAText2"/>
                                            <ctaColor
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="CTA Color"
                                                    name="./featureCTAColor2">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <default
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Default (Default)"
                                                            value=""/>
                                                    <pink
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Pink"
                                                            value="beej-button--pink"/>
                                                    <blue
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Blue"
                                                            value="beej-button--blue"/>
                                                    <purple
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Purple"
                                                            value="beej-button--purple"/>
                                                    <orange
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Orange"
                                                            value="beej-button--orange"/>
                                                </items>
                                            </ctaColor>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </featureTwo>
                    <featureThree
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Feature 3"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <featureIcon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                    allowUpload="{Boolean}false"
                                                    autoStart="{Boolean}false"
                                                    class="cq-droptarget"
                                                    fieldLabel="Feature Icon"
                                                    fieldDescription="Drop an icon from the DAM here"
                                                    fileNameParameter="./primaryLogo"
                                                    fileReferenceParameter="./featureIcon3"
                                                    mimeTypes="[image]"
                                                    multiple="{Boolean}false"
                                                    name="./file"
                                                    title="Primary Logo Upload"
                                                    useHTML5="{Boolean}true"/>
                                            <featureTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Feature Title"
                                                    name="./featureTitle3"/>
                                            <featureText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    fieldLabel="Feature Text"
                                                    name="./featureText3"/>
                                            <ctaURL
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldLabel="Feature CTA Path"
                                                    name="./featureCTAPath3"
                                                    rootPath="/content"/>
                                            <ctaText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Feature CTA Text"
                                                    name="./featureCTAText3"/>
                                            <ctaColor
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="CTA Color"
                                                    name="./featureCTAColor3">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <default
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Default (Default)"
                                                            value=""/>
                                                    <pink
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Pink"
                                                            value="beej-button--pink"/>
                                                    <blue
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Blue"
                                                            value="beej-button--blue"/>
                                                    <purple
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Purple"
                                                            value="beej-button--purple"/>
                                                    <orange
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Orange"
                                                            value="beej-button--orange"/>
                                                </items>
                                            </ctaColor>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </featureThree>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>